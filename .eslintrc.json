  {
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "root": true,
    "rules": {
      "no-console": "off",
      "vue/html-self-closing": [
        "error",
        {
          "html": {
            "component": "always",
            "normal": "always",
            "void": "always"
          }
        }
      ]
    }
  }